{% extends 'base.html.twig' %}

{% block title %}PhotoVoiture{% endblock %}

{% block body %}
    <h1>PhotoVoiture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ photo.id }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ photo.url }}</td>
